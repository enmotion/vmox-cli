import { Command } from "commander";
const program = new Command();

program.version('1.0.0');

program
.command('t <name>')
.description('Print hello vmo')
.action((name)=>{
    console.log('hello '+name);
})

program
.command('s <name>')
.description('Print hello vmo')
.action((name)=>{
    console.log('shit '+name);
})